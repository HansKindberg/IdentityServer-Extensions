@model DeviceForm
<form asp-action="UserCode" asp-route-ui_locales="@Context.Request.Query[QueryStringKeys.UiLocales]">
	<div class="mb-3">
		<label asp-for="UserCode"></label>
		<input asp-for="UserCode" autofocus="autofocus" class="form-control" />
	</div>
	<div class="text-end">
		<button class="btn btn-primary" type="submit">
			@Localizer.GetString("Submit")
		</button>
	</div>
</form>