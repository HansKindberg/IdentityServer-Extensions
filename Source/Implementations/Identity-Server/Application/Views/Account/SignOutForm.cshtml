@model SignOutForm
@if(Model != null)
{
	<form asp-action="SignOut" asp-controller="Account" asp-route-ui_locales="@Context.Request.Query[QueryStringKeys.UiLocales]" class="d-inline ms-2">
		<input asp-for="Id" type="hidden" />
		<button autofocus="autofocus" class="btn btn-primary" type="submit">@Localizer.GetString("Accept")</button>
	</form>
}