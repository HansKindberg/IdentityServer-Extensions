<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<location path="identity">
		<system.webServer>
			<handlers>
				<add name="aspNetCore" modules="AspNetCoreModuleV2" path="*" resourceType="Unspecified" verb="*" />
			</handlers>
			<security>
				<access sslFlags="Ssl, SslNegotiateCert, SslRequireCert" />
			</security>
		</system.webServer>
	</location>
</configuration>